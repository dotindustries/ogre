{
  "name": "@dotinc/ogre",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotindustries/ogre.git"
  },
  "version": "0.3.0",
  "description": "Git-like repository for in-memory object versioning",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "nyc --reporter=lcov ava",
    "test.watch": "ava --watch",
    "coverage:html": "nyc report --reporter=html"
  },
  "ava": {
    "timeout": "60s",
    "files": [
      "./src/**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "author": "JÃ¡nos Veres @nadilas",
  "license": "MIT",
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "ava": "^4.2.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
