{
  "name": "versioning",
  "version": "0.0.0",
  "private": true,
  "description": "Version Control library",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "test": "ava",
    "test.watch": "ava --watch"
