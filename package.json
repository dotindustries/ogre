{
  "name": "versioning",
  "version": "0.0.0",
  "private": true,
  "description": "Version Control library",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "test": "ava ./src/*.test.ts",
    "test.watch": "ava --watch ./src/*.test.ts"
  },
  "ava": {
    "timeout": "60s",
    "concurrency": 1,
    "typescript": {
      "rewritePaths": {
        "src/": "lib/"
      },
      "compile": "tsc"
    }
  },
  "author": "JÃ¡nos Veres @nadilas",
  "license": "MIT",
  "dependencies": {
    "object-hash": "^3.0.0"
  },
  "devDependencies": {
    "typescript": "^4.5.5",
    "@types/object-hash": "^2.2.1",
    "@types/uuid": "^8.3.4",
    "uuid": "^8.3.2",
    "ava": "^4.0.1",
    "@ava/typescript": "^3.0.1",
    "config": "*"
  }
}
