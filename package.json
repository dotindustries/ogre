{
  "name": "@dotindustries/ogre",
  "version": "0.0.0",
  "private": false,
  "description": "In-memory git-like repository for objects",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "test": "nyc ava",
    "test.watch": "ava --watch",
    "coverage:html": "nyc report --reporter=html",
    "coverage:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "coverage": "nyc report"
  },
  "ava": {
    "timeout": "60s",
    "files": [
      "./src/**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "author": "JÃ¡nos Veres @nadilas",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^4.5.5",
    "@types/uuid": "^8.3.4",
    "@types/node": "^17.0.21",
    "uuid": "^8.3.2",
    "ava": "^4.0.1",
    "@ava/typescript": "^3.0.1",
    "nyc": "^15.1.0",
    "coveralls": "^3.1.1",
    "ts-node": "^10.7.0"
  }
}
