{
  "name": "ogre",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "JÃ¡nos Veres",
    "email": "janos@dot.industries"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --cache-dir=.turbo",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "release": "auto shipit"
  },
  "license": "MIT",
  "devDependencies": {
    "@auto-it/conventional-commits": "^11.1.2",
    "@auto-it/microsoft-teams": "^11.0.7",
    "@auto-it/npm": "^11.0.7",
    "@auto-it/released": "^11.0.7",
    "auto": "^11.0.7",
    "dotenv-cli": "^7.3.0",
    "prettier": "^3.2.5",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=17.0.0"
  },
  "auto": {
    "plugins": [
      [
        "npm",
        {
          "monorepoChangelog": true
        }
      ],
      "released",
      "conventional-commits",
      "microsoft-teams"
    ]
  },
  "packageManager": "npm@8.5.1"
}
