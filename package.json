{
  "name": "ogre",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "JÃ¡nos Veres",
    "email": "janos@dot.industries"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "release": "auto shipit -vv"
  },
  "license": "MIT",
  "devDependencies": {
    "@auto-it/conventional-commits": "^10.37.6",
    "@auto-it/microsoft-teams": "^10.37.1",
    "@auto-it/npm": "^10.34.1",
    "@auto-it/released": "^10.37.1",
    "auto": "^10.34.1",
    "dotenv-cli": "^5.0.0",
    "lerna": "^4.0.0",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=17.0.0"
  },
  "auto": {
    "plugins": [
      [
        "npm",
        {
          "monorepoChangelog": true
        }
      ],
      "released",
      "conventional-commits",
      "microsoft-teams"
    ]
  },
  "packageManager": "npm@8.5.1"
}
