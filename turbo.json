{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "lib/**"]
    },
    "test": {
      "cache": false,
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test.watch": {
      "cache": false
    },
    "coverage:html": {
      "dependsOn": ["^test"],
      "outputs": ["coverage/**"]
    },
    "coverage": {
      "dependsOn": ["^test"],
      "outputs": [".nyc_output/**"]
    }
  }
}
